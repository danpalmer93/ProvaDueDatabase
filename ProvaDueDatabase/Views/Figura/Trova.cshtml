@model IEnumerable<FiguraDto>
@{
}


<div>
    <table>
        <thead>
            <tr>
                <th>
                    ID
                </th>
                <th>
                    binario
                </th>
                <th>
                    descrizione
                </th>
                <th>
                    filename
                </th>
            </tr>
        </thead>
        <tbody>
            @{
                foreach (var i in Model)
                {
                    <tr>
                        <td>
                            @i.Id
                        </td>
                        <td>
                            @i.Immagine
                        </td>
                        <td>
                            @i.Descrizione
                        </td>
                    </tr>
                }
            }


        </tbody>
    </table>


</div>
